{
    "$schema": "http://www.lavishsoft.com/schema/isb2.json",
    "name": "Variable Follow Me",
    "hotkeySheets": [
        {
            "name": "Base Hotkeys",
            "enable": true,
            "hotkeys": [
                {
                    "name": "Follow Me",
                    "keyCombo": "Alt+F",
                    "inputMapping": {
                        "type": "mappable",
                        "sheet": "Base Hotkeys",
                        "name": "Follow Me"
                    }
                }
            ]
        }
    ],
    "gameKeyBindings": [
        {
            "name": "Follow Slot 1",
            "keyCombo": "Shift+F11"
        },
        {
            "name": "Follow Slot 2",
            "keyCombo": "Ctrl+F11"
        }
    ],
    "mappableSheets": [
        {
            "name": "Base Hotkeys",
            "mappables": [
                {
                    "name": "Follow Me",
                    "steps": [
                        {
                            "actions": [
                                {
                                    "type": "Game Key Binding",
                                    "name": "Follow Slot {SLOT}",
                                    "target": "all other"
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}