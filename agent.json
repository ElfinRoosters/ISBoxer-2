{
    "$schema": "http://www.lavishsoft.com/schema/agent.json",
    "name": "ISBoxer 2022",
    "version": "20220923.1",
    "minimumBuild": 6997,
    "platforms": {
        "inner space uplink": {
            "eventHandlers": {
                "onAgentStartup": {
                    "code": [
                        "run ISB2022.Uplink.iss"
                    ]
                },
                "onAgentShutdown": {
                    "code": [                        
                        "endscript ISB2022.Uplink"
                    ]
                }
            }
        },
        "inner space session": {
            "eventHandlers": {
                "onAgentStartup": {
                    "code": [
                        "run ISB2022.Session.iss"
                    ]
                },
                "onAgentShutdown": {
                    "code": [                        
                        "endscript ISB2022.Session"
                    ]
                }
            }
        }

    }
}