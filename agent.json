{
    "$schema": "http://www.lavishsoft.com/schema/agent.json",
    "name": "ISBoxer 2",
    "version": "20221017.1",
    "minimumBuild": 7035,
    "platforms": {
        "inner space uplink": {
            "eventHandlers": {
                "onAgentStartup": {
                    "code": [
                        "run ISB2.Uplink.iss"
                    ]
                },
                "onAgentShutdown": {
                    "code": [                        
                        "endscript ISB2.Uplink"
                    ]
                }
            }
        },
        "inner space session": {
            "eventHandlers": {
                "onAgentStartup": {
                    "code": [
                        "run ISB2.Session.iss"
                    ]
                },
                "onAgentShutdown": {
                    "code": [                        
                        "endscript ISB2.Session"
                    ]
                }
            }
        }

    }
}