{
    "$schema": "http://www.lavishsoft.com/schema/lgui2Package.json",
    "skin":"ISBoxer 2",
    "templates": {
            "isb2.editorContexts":{
                "contexts":[
                {
                    "name":"main",
                    "title":"",
                    "subItems":[
                        {
                            "name":"Profile",
                            "item":"EditingItem",
                            "context":"profile"
                        },
                        {
                            "name":"Characters",
                            "list":{
                                "source":"Editor.Editing.Characters",
                                "viewTemplate":"isb2.characterView",
                                "context":"character"
                            }
                        },
                        {
                            "name":"Teams",
                            "list":{
                                "source":"Editor.Editing.Teams",
                                "viewTemplate":"isb2.teamView",
                                "context":"team"
                            }
                        },
                        {
                            "name":"Window Layouts",
                            "list":{
                                "source":"Editor.Editing.WindowLayouts",
                                "viewTemplate":"isb2.windowLayoutView",
                                "context":"windowLayout"
                            }
                        },
                        {
                            "name":"Click Bars",
                            "list":{
                                "source":"Editor.Editing.ClickBars",
                                "viewTemplate":"isb2.clickBarView",
                                "context":"clickBar"
                            }
                        },
                        {
                            "name":"Hotkey Sheets",
                            "list":{
                                "source":"Editor.Editing.HotkeySheets",
                                "viewTemplate":"isb2.hotkeySheetView",
                                "context":"hotkeySheet"
                            }
                        },
                        {
                            "name":"Game Key Bindings",
                            "list":{
                                "source":"Editor.Editing.GameKeyBindings",
                                "viewTemplate":"isb2.gameKeyBindingView",
                                "context":"gameKeyBinding"
                            }
                        },
                        {
                            "name":"Broadcast Profiles",
                            "list":{
                                "source":"Editor.Editing.BroadcastProfiles",
                                "viewTemplate":"isb2.broadcastProfileView",
                                "context":"broadcastProfile"
                            }
                        },
                        {
                            "name":"Mappable Sheets",
                            "list":{
                                "source":"Editor.Editing.MappableSheets",
                                "viewTemplate":"isb2.mappableSheetView",
                                "context":"mappableSheet"
                            }
                        },
                        {
                            "name":"Game Macro Sheets",
                            "list":{
                                "source":"Editor.Editing.GameMacroSheets",
                                "viewTemplate":"isb2.gameMacroSheetView",
                                "context":"gameMacroSheet"
                            }
                        },
                        {
                            "name":"Variables",
                            "list":{
                                "source":"Editor.Editing.Variables",
                                "viewTemplate":"isb2.variableView",
                                "context":"variable"
                            }
                        },
                        {
                            "name":"VFX Sheets",
                            "list":{
                                "source":"Editor.Editing.VFXSheets",
                                "viewTemplate":"isb2.vfxSheetView",
                                "context":"vfxsheet"
                            }
                        },
                        {
                            "name":"Click Bar Templates",
                            "list":{
                                "source":"Editor.Editing.ClickBarTemplates",
                                "viewTemplate":"isb2.commonView",
                                "context":"clickBarTemplate"
                            }
                        },
                        {
                            "name":"Image Sheets",
                            "list":{
                                "source":"Editor.Editing.ImageSheets",
                                "viewTemplate":"isb2.commonView",
                                "context":"imageSheet"
                            }
                        }
                    ]
                },
                {
                    "name":"profile",
                    "title":"Profile: ${EditingItem.Name}",
                    "subItems":[
                        {
                            "name":"General",
                            "template":"isb2.profileEditor.General"
                        },
                        {
                            "name":"Configuration Builders",
                            "list":{
                                "source":"EditingItem.Builders",
                                "viewTemplate":"isb2.builderView",
                                "context":"builder"
                            }
                        }
                    ]
                },
                {
                    "name":"builder",
                    "title":"Builder: ${EditingItem.Get[name]}",
                    "subItems":[
                        {
                            "name":"General",
                            "template":"isb2.builderEditor.General"
                        },
                        {
                            "name":"Team",
                            "context":"team",
                            "item":"This.EditingItem.Get[team]"
                        },
                        {
                            "name":"Slot",
                            "context":"slot",
                            "item":"This.EditingItem.Get[slot]"
                        },
                        {
                            "name":"Character",
                            "context":"character",
                            "item":"This.EditingItem.Get[character]"
                        }
                    ]
                },
                {
                    "name":"character",
                    "title":"Character: ${EditingItem.Get[name]}",
                    "subItems":[
                        {
                            "name":"General",
                            "template":"isb2.characterEditor.General"
                        }
                    ]
                },
                {
                    "name":"team",
                    "title":"Team: ${EditingItem.Get[name]}",
                    "subItems":[
                        {
                            "name":"General",
                            "template":"isb2.teamEditor.General"
                        },
                        {
                            "name":"Configuration Builders",
                            "template":"isb2.teamEditor.ConfigurationBuilders",
                            "init":"InitConfigurationBuilders"
                        },
                        {
                            "name":"Slots",
                            "list":{
                                "source":"EditingItem.Get[slots]",
                                "viewTemplate":"isb2.slotView",
                                "context":"slot"
                            }    
                        },
                        {
                            "name":"Hotkey Sheets",
                            "list":{
                                "source":"EditingItem.Get[hotkeySheets]",
                                "viewTemplate":"isb2.stringView",
                                "context":"hotkeySheetName",
                                "new":""
                            }
                        },
                        {
                            "name":"Mappable Sheets",
                            "list":{
                                "source":"EditingItem.Get[mappableSheets]",
                                "viewTemplate":"isb2.stringView",
                                "context":"mappableSheetName",
                                "new":""
                            }
                        },
                        {
                            "name":"Click Bars",
                            "list":{
                                "source":"EditingItem.Get[clickBars]",
                                "viewTemplate":"isb2.clickBarView",
                                "context":"clickBarName",
                                "new":""
                            }
                        },
                        {
                            "name":"Game Macro Sheets",
                            "list":{
                                "source":"EditingItem.Get[gameMacroSheets]",
                                "viewTemplate":"isb2.stringView",
                                "context":"gameMacroSheetName",
                                "new":""
                            }
                        },
                        {
                            "name":"Virtual Mappables",
                            "list":{
                                "source":"EditingItem.Get[virtualMappables]",
                                "viewTemplate":"isb2.virtualMappableView"
                            }
                        }
                    ],
                    "dragDrop":{
                        "clickBar":{
                            "type":"add",
                            "inProperty":"name",
                            "outProperty":"clickBars"
                        },
                        "clickBarName":{
                            "type":"add",
                            "outProperty":"clickBars"
                        },
                        "hotkeySheet":{
                            "type":"add",
                            "inProperty":"name",
                            "outProperty":"hotkeySheets"
                        },
                        "hotkeySheetName":{
                            "type":"add",
                            "outProperty":"hotkeySheets"
                        },
                        "mappableSheet":{
                            "type":"add",
                            "inProperty":"name",
                            "outProperty":"mappableSheets"
                        },
                        "mappableSheetName":{
                            "type":"add",
                            "outProperty":"mappableSheets"
                        },
                        "gameMacroSheet":{
                            "type":"add",
                            "inProperty":"name",
                            "outProperty":"gameMacroSheets"
                        },
                        "gameMacroSheetName":{
                            "type":"add",
                            "outProperty":"gameMacroSheets"
                        },
                        "windowLayout":{
                            "type":"copy",
                            "inProperty":"name",
                            "outProperty":"windowLayout"
                        }
                    }
                },
                {
                    "name":"windowLayout",
                    "title":"Window Layout: ${EditingItem.Get[name]}",
                    "subItems":[
                        {
                            "name":"General",
                            "template":"isb2.windowLayoutEditor.General"
                        },
                        {
                            "name":"Regions",
                            "list":{
                                "source":"EditingItem.Get[regions]",
                                "viewTemplate":"isb2.windowLayout.regionView",
                                "context":"windowLayout.region"
                            }            
                        }
                    ]
                },
                {
                    "name":"mappableSheet",
                    "title":"Mappable Sheet: ${EditingItem.Get[name]}",
                    "subItems":[
                        {
                            "name":"General",
                            "template":"isb2.mappableSheetEditor.General"
                        },
                        {
                            "name":"Mappables",
                            "list":{
                                "source":"EditingItem.Get[mappables]",
                                "viewTemplate":"isb2.mappableView",
                                "context":"mappable"
                            }            
                        }
                    ]
                },
                {
                    "name":"hotkeySheet",
                    "title":"Hotkey Sheet: ${EditingItem.Get[name]}",
                    "subItems":[
                        {
                            "name":"General",
                            "template":"isb2.hotkeySheetEditor.General"
                        },
                        {
                            "name":"Hotkeys",
                            "list":{
                                "source":"EditingItem.Get[hotkeys]",
                                "viewTemplate":"isb2.hotkeyView",
                                "context":"hotkey"
                            }    
                        }
                    ]
                },
                {
                    "name":"hotkey",
                    "title":"Hotkey: ${EditingItem.Get[name]}",
                    "template":"isb2.hotkeyEditor.General"
                },
                {
                    "name":"action",
                    "template":"isb2.actionEditor.General"
                },
                {
                    "name":"mappable",
                    "title":"Mappable: ${EditingItem.Get[name]}",
                    "subItems":[
                        {
                            "name":"General",
                            "template":"isb2.mappableEditor.General"
                        },
                        {
                            "name":"Steps",
                            "list":{
                                "source":"EditingItem.Get[steps]",
                                "viewTemplate":"isb2.mappable.stepView",
                                "context":"mappableStep"
                            }    
                        }
                    ]
                },
                {
                    "name":"mappableStep",
                    "title":"Step",
                    "subItems":[
                        {
                            "name":"General",
                            "template":"isb2.mappableStepEditor.General"
                        },
                        {
                            "name":"Actions",
                            "list":{
                                "source":"EditingItem.Get[actions]",
                                "viewTemplate":"isb2.mappable.actionView",
                                "context":"action"
                            }    
                        }
                    ]
                },
                {
                    "name":"gameMacroSheet",
                    "title":"Game Macro Sheet: ${EditingItem.Get[name]}",
                    "subItems":[
                        {
                            "name":"General",
                            "template":"isb2.gameMacroSheetEditor.General"
                        },
                        {
                            "name":"Macros",
                            "list":{
                                "source":"EditingItem.Get[macros]",
                                "viewTemplate":"isb2.gameMacroView",
                                "context":"gameMacro"
                            }    
                        }
                    ]                                    
                },
                {
                    "name":"gameMacro",
                    "title":"Game Macro: ${EditingItem.Get[name]}",
                    "template":"isb2.gameMacroEditor.General"
                },
                {
                    "name":"clickBar",
                    "title":"Click Bar: ${EditingItem.Get[name]}",
                    "subItems":[
                        {
                            "name":"General",
                            "template":"isb2.clickBarEditor.General"
                        },
                        {
                            "name":"Template",
                            "template":"isb2.clickBarEditor.Template"
                        },
                        {
                            "name":"Button Layout",
                            "template":"isb2.clickBarEditor.ButtonLayout"
                        }
                    ]                                    
                },
                {
                    "name":"slot",
                    "title":"Slot",
                    "subItems":[
                        {
                            "name":"General",
                            "template":"isb2.slotEditor.General"
                        },
                        {
                            "name":"VFX Sheets",
                            "list":{
                                "source":"EditingItem.Get[vfxSheets]"
                            }
                        },
                        {
                            "name":"Game Key Bindings",
                            "list":{
                                "source":"EditingItem.Get[gameKeyBindings]",
                                "viewTemplate":"isb2.gameKeyBindingInstanceView",
                                "context":"gameKeyBindingInstance"
                            }
                        }
                    ],
                    "dragDrop":{
                        "character":{
                            "type":"copy",
                            "inProperty":"name",
                            "outProperty":"character"
                        }
                    }
                }
            ]
        }
    }
}